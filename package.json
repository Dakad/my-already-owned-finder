{
    "name": "my-already-owned-finder",
    "version": "1.0.0",
    "author": "Dakad",
    "description": "Little WebExtension to not download twice the same item",
    "homepage": "https://github.com/Dakad/my-finder-already-owned#readme",
    "repository": "git+https://github.com/Dakad/my-finder-already-owned.git",
    "license": "MIT",
    "private": true,
    "main": "background.js",
    "scripts": {
        "test": "karma start",
        "test:debug": "karma start --no-single-run --auto-watch",
        "web-ext": "web-ext run -s ./addon --start-url about:debugging --browser-console",
        "build" : "web-ext build -s ./addon -a ./build/Release/"
    },
    "bugs": {
        "url": "https://github.com/Dakad/my-finder-already-owned/issues"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "expect.js": "^0.3.1",
        "karma": "^3.1.1",
        "karma-firefox-launcher": "^1.1.0",
        "karma-mocha": "^1.3.0",
        "mocha": "^5.2.0",
        "sinon-chrome": "^2.3.2",
        "yarn": "^1.12.3"
    }
}